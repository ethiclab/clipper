//MACROS
CLIPFLAGS=  -ES2 -N -A -M -W -DDEF_GRAPHICS -I.\HEADER
C_FLAGS= -c -W3 -Oalt -AL -Gs -FPa -Zl -Gh
LINK_PARMS=@PPOSGRFX.LNK

// PRG file
.prg.obj:
   clipper $< $(CLIPFLAGS)  

// "C" file
.c.obj:
   cl $(C_FLAGS) $<

// OBJ file
.obj.exe:
   BLINKER $(LINK_PARMS) 
      
   
// NETWARE based (LanSmart, Novell, Lantastic)

// clipper $< $(CLIPFLAGS) 

// TCP_IP using DOS platform

// clipper $< $(CLIPFLAGS) /dTCP_IP

// TCP_IP using UNIX platform

// clipper $< $(CLIPFLAGS) /dTCP_IP /dUNIX

